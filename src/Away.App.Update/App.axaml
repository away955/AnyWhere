<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Away.App.Update.App"
             RequestedThemeVariant="Dark">

	<Application.Resources>
		<ResourceDictionary>
			<SolidColorBrush x:Key="AwayBackground"  Color="#FF212121" />
			<SolidColorBrush x:Key="AwayPaper"  Color="#FF303030" />
			<SolidColorBrush x:Key="AwayCardBackground"  Color="#FF424242" />
			<SolidColorBrush x:Key="AwayToolBarBackground"  Color="#FF303030" />
		</ResourceDictionary>
	</Application.Resources>

	<Application.Styles>
		<FluentTheme />
	</Application.Styles>
</Application>